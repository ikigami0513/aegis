namespace System {
    func clear() { 
        return io_clear()
    }

    func write(str) { 
        return io_write(str) 
    }

    func env(key) {
        return sys_env(key)
    }

    func args() {
        return __ARGS__
    }

    func fail(msg) {
        return sys_fail(msg)
    }

    func exit(code) { 
        return sys_exit(code) 
    }
}
