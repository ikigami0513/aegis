print "--- TEST POO RUST ---"

class Animal {
    init(name) {
        this.name = name
    }

    parler() {
        print this.name + " fait du bruit."
    }
}

class Chien extends Animal {
    parler() {
        print this.name + " aboie : Wouf !"
    }
    
    renommer(new_name) {
        // Test de SetAttr sur 'this'
        this.name = new_name
    }
}

var medor = new Chien("Medor")
medor.parler()

print "Changement de nom..."
medor.renommer("Brutus")
medor.parler()

// Test attribut direct
print "Lecture attribut direct : " + medor.name